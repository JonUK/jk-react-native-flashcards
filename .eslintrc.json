{
  "plugins": [
    "react",
    "react-native"
  ],
  "env": {
    "react-native/react-native": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "rules": {
    "semi": ["error" , "always" ],
    "quotes": ["error", "single"],
    "no-duplicate-imports": "error",
    "comma-dangle": ["error", "never"]
  }

}
